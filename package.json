  ],
  "license": "UNLICENSED",
  "private": true,
  "ewi": {
    "masterName": "diabetes-education"
  },
  "engines": {
    "node": "10.x"
  },
  "scripts": {
    "build": "nuxt build",
    "build:watch": "nodemon -e vue,scss,js --ignore api --ignore .nuxt --ignore protected --ignore public --ignore routes --ignore test --exec 'nuxt generate --no-lock'",
    "coverage": "nyc npm test",
    "develop": "koa-start-local",
    "develop:watch": "nodemon --watch api --watch routes -e js --exec 'koa-start-local'",
    "heroku-postbuild": "npm run build",
    "lint": "eslint . --ext .js --ext .vue",
    "masterbuild": "global-pkg-master-builder",
    "snyk:wizard": "snyk auth 63d1a597-f300-44d1-ace4-f4ca4872afc7 && snyk wizard",
    "snyktest": "snyk auth 63d1a597-f300-44d1-ace4-f4ca4872afc7 && snyk test",
    "start": "koa-start",
    "test": "npm run test:api && npm run test:components",
    "test:api": "mocha --opts mocha.opts",
    "test:clear-cache": "jest --clearCache",
    "test:components": "jest",
    "test:integrations": "mocha \"test/integrations/**/*.test.js\"",
    "test:update": "jest --updateSnapshot",
    "watch": "npm-run-all --parallel build:watch develop:watch"
  },
  "dependencies": {
    "axios": "^0.18.1",
    "debug": "^3.1.0",
    "ejs": "^2.6.1",
    "global-auth-middleware": "1.1.1",
    "global-pkg-authed-assets": "^1.1.0",
    "global-pkg-koa-starter": "^1.0.6",
    "koa-bodyparser": "^4.2.1",
    "koa-route": "^3.2.0",
    "lodash": "^4.17.11",
    "request": "^2.88.0",
    "validator": "^10.10.0"
  },
  "devDependencies": {
    "@nuxtjs/dotenv": "^1.3.0",
    "@nuxtjs/google-tag-manager": "^2.1.4",
    "@nuxtjs/style-resources": "^0.1.1",
    "@vue/babel-helper-vue-jsx-merge-props": "^0.1.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.1",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-jest": "^24.5.0",
    "babel-loader": "^8.0.5",
    "bootstrap": "~4.2.1",
    "chai": "^4.0.2",
    "chai-as-promised": "^7.1.1",
    "cheerio": "^1.0.0-rc.3",
    "eslint": "^5.4.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^4.0.5",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-vue": "^4.0.0",
    "global-pkg-ewi-vue-comps": "^1.4.0",
    "html-loader": "^0.5.5",
    "jest": "^24.5.0",
    "jest-serializer-vue": "^2.0.2",
    "koa": "^2.6.2",
    "koa-compose": "^4.1.0",
    "lilly-design-system": "^3.1.0",
    "mocha": "^5.2.0",
    "nock": "^10.0.6",
    "node-dir": "^0.1.17",
    "node-sass": "^4.9.4",
    "nodemon": "^1.18.8",
    "npm-run-all": "^4.1.5",
    "nuxt": "2.5.1",
    "nyc": "^12.0.2",
    "sass-loader": "^7.1.0",
    "snyk": "^1.94.0",
    "supertest": "^3.3.0",
    "vue": "^2.6.10",
    "vue-jest": "^3.0.4",
    "vue-svg-loader": "^0.11.0"
  }
